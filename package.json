{
  "name": "serverless-python-demo",
  "version": "1.0.0",
  "scripts": {
    "localstack:start": "docker run -d --rm --name localstack -p 127.0.0.1:4566:4566 -p 127.0.0.1:4510-4559:4510-4559 -v /var/run/docker.sock:/var/run/docker.sock localstack/localstack",
    "localstack:stop": "docker stop localstack",
    "dev:start": "serverless deploy --stage offline",
    "dev:stop": "serverless remove --stage offline"
  },
  "devDependencies": {
    "serverless": "^3.39.0",
    "serverless-localstack": "^1.3.1"
  }
}